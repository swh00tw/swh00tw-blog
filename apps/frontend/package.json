{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint-fix": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"ts:check": "tsc --noEmit",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"update-types": "eval $(grep '^PUBLIC_SUPABASE_ID' .env.local) && npx supabase gen types typescript --project-id ${PUBLIC_SUPABASE_ID} --schema public > src/lib/db/types.ts"
	},
	"lint-staged": {
		"*.{js,ts,svelte,css,scss,postcss,json}": [
			"prettier --write --plugin-search-dir=.",
			"prettier --check --plugin-search-dir=."
		],
		"*.{js,ts,svelte}": "eslint --cache --fix"
	},
	"devDependencies": {
		"@ethercorps/sveltekit-og": "^2.0.1",
		"@fontsource-variable/work-sans": "^5.0.9",
		"@fontsource/dm-sans": "^5.0.8",
		"@fontsource/jetbrains-mono": "^5.0.17",
		"@histoire/plugin-svelte": "^0.16.3",
		"@supabase/supabase-js": "^2.39.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"histoire": "^0.16.2",
		"lucide-svelte": "^0.263.0",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"shiki": "^0.14.5",
		"supabase": "^1.123.4",
		"svelte": "^4.2.7",
		"svelte-check": "^3.4.3",
		"svelte-loading-spinners": "^0.3.4",
		"svooltip": "^0.7.4",
		"tailwind-merge": "^1.14.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"zod": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"sharp": "^0.33.1"
	}
}
